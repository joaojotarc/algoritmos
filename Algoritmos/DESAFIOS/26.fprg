                            <output expression="&quot;Esse tri&#226;ngulo &#233; is&#243;celes&quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="a&lt;b+c &amp;&amp; b&lt;a+c &amp;&amp; c&lt;a+b">
                                <then>
                                    <output expression="&quot;Esse tri&#226;ngulo &#233; ret&#226;ngulo&quot;" newline="True"/>
                                </then>
